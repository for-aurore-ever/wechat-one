/* pages/score/score.wxss */
.Countainer{
  display: flex;
  flex-wrap: wrap;
  height: 100%;
   border: 2rpx solid rgba(245, 245, 245, 1);
}
.photo image{
  /* 开启绝对定位 */
  position: absolute;
  /* 设置层级 */
  z-index: 2;
  height: 830rpx;
  width: 600rpx;
  left: 0;
  top: 0;
  right: 0;
  margin: auto;
  margin-top: 160rpx;
}
.background image{
  display: flex;
  margin: 0 auto;
  height: 1000rpx;
  padding: 30rpx 30rpx;
  position: relative;
  width: 700rpx;
  margin-bottom: 80rpx;
}